@page "/RedirectToLogin"
@page "/RedirectToLogin/{loggedInError:bool}"

@code {
    [Inject]
    private NavigationManager navMan { get; set; }

    [Parameter]
    public bool loggedInError { get; set; } = false;

    protected override void OnAfterRender(bool firstRender)
    {
        navMan.NavigateTo("/auth/logout/" + loggedInError, true);
    }
}